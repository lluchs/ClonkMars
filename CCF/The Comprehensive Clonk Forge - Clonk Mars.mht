/*--Allgemeines Aussehen--*/

body {
	background-color: white;
	background-image: none;
	color: #2b2b2b;
}

table.project {
	border:1px solid #313131; border-spacing:2px;
	background-color:#e9e9e9;
	font-family:Arial;
	font-size:11pt;
	width:85%;
}

table.usercomment {
	border:1px solid #313131; border-spacing:2px;
	background-image:none;
	background-color:#e9e9e9;
	width:85%;
}

tr.listtitle td, tr.listtitle th {
	background-image:none;
	background-color:#e9e9e9;
	border-bottom-color: #313131;
}

table.comment {
	border:2px solid #313131;
	border-spacing:0px;
	width:85%;
}

td.usercomment {
	border:none;
	background-color:#e9e9e9;
}
f
td.comment {
	border:3px solid #313131;
}

table {
	border:1px solid #313131;
	background-color:#e9e9e9;
}

tr.prhighlight{
	font-family:Arial;
	background-color:#e9e9e9;
}

h1 {
	font-family:Arial;
	font-size:14pt;
	margin-top:0px;
}

h2 {
	font-family:Arial;
	font-size:12pt;
	margin-top:0px;
}

a:link {
color:#32527A;
text-decoration:none;
}
a:visited {
color:#999999;
text-decoration:none;
}
a:hover {
color:#063E3F;
text-decoration:underline;
}

#title {
	border:6px solid #aaaaaa;
	background-color: #666666;
}

#title table {
	background-color: #e4e5f2;
}

.adminbar td {
	background-color: #e4e5f2;
}

table.vote {
	border:1px solid #313131;
	margin-bottom: 1em;
}

tr.voteh td {
	border-bottom: 1px solid #313131;
	padding: 2px;
}

tr.voteb td {
	padding: 2px;
}

hr {
	width:60%;
	height:1px;
	border:1px solid #999999;
}

/* Konzepte */

.kbox {
	margin-bottom: 1em;
	width: 100%;
	min-height: 140px;
}

.konzept {
	margin-right: 200px;
}

/* Sprite fr Bilder */

.sprite {
	background: url('http://clonkforge.net/projects/project1125/Konzepte.jpg');
	width: 176px;
	height: 135px;
	margin-right: 5px;
	float: right;
}

	.clunker         { background-position: 0px 0px; }
	.materialien { background-position: 0px -136px; }
	.siedeln         { background-position: 0px -273px; }
	.forschen       { background-position: 0px -411px; }
	.jetbelt         { background-position: 0px -550px; }

